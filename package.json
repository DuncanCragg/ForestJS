  "description": "Functional Observer REST",
  "author": "Duncan Cragg <forest@cilux.org> (http://duncan-cragg.org)",
  "license": "MIT",
  "keywords": [
    "forest",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DuncanCragg/forestjs.git"
  },
  "bugs": {
    "url": "https://github.com/DuncanCragg/forestjs/issues"
  },
  "homepage": "https://github.com/DuncanCragg/forestjs#readme",
  "scripts": {
    "build": "babel src examples --out-dir lib --presets es2015 --plugins add-module-exports,transform-react-jsx,transform-class-properties",
